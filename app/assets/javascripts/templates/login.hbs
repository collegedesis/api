<div class="main">
  <h2>Log In</h2>
  <form>
    Email:
    {{view Ember.TextField type="email" valueBinding="email"}}
    Password:
    {{view Ember.TextField type="password" valueBinding="password"}}

    <a {{action login}}>Login</a>
  </form>
    Or {{#linkTo "newUser"}}Sign Up{{/linkTo}}
</div>